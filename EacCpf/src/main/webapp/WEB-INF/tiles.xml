<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
    <definition name="webFormEacCpf" template="/jsp/templates/eaccpf.jsp">
        <put-attribute name="title" value="EAC-CPF web form" />
        <put-attribute name="body" value="/jsp/templates/blank.jsp" />
        <put-attribute name="maincss" value="/css/cssnew.css"/>
        <put-list-attribute name="jsfilesInternal">
            <add-attribute type="string" value="/js/jquery/jquery-1.8.2.min.js"/>
            <add-attribute type="string" value="/js/breadcrumb.js"/>
        </put-list-attribute>
        <put-list-attribute name="cssfilesInternal"/>
    </definition>

    <definition name="index" extends="webFormEacCpf">
        <put-attribute name="body" value="/jsp/choosetype.jsp" />
<!--        <put-attribute name="body" value="/jsp/webformEACCPF.jsp" />-->
        <put-list-attribute name="jsfilesInternal">
            <add-attribute type="string" value="/js/jquery/jquery-1.8.2.min.js"/>
            <add-attribute type="string" value="/js/breadcrumb.js"/>
            <add-attribute type="string" value="/js/eaccpf/eaccpf.js"/>
        </put-list-attribute>
    </definition>

    <definition name="edit" extends="webFormEacCpf">
        <put-attribute name="body" value="/jsp/webformEACCPF.jsp" />
        <put-list-attribute name="jsfilesInternal">
            <add-attribute type="string" value="/js/jquery/jquery-1.8.2.min.js"/>
            <add-attribute type="string" value="/js/breadcrumb.js"/>
            <add-attribute type="string" value="/js/eaccpf/eaccpf.js"/>
        </put-list-attribute>
    </definition>

</tiles-definitions>
