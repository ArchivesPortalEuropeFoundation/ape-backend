<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="otherPersistence">
        <class>eu.apenet.oaiserver.config.other.vo.EadObject</class>
        <class>eu.apenet.oaiserver.config.other.vo.ResumptionToken</class>
        <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect" />
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver" />
            <property name="hibernate.cache.use_second_level_cache" value="false" />
            <property name="hibernate.cache.use_query_cache" value="false" />
            <property name="hibernate.show_sql" value="true" />

            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:10111/oaiphp" />
            <property name="javax.persistence.jdbc.user" value="oaiphp" />
            <property name="javax.persistence.jdbc.password" value="$Talmac!" />
        </properties>
    </persistence-unit>
</persistence>

