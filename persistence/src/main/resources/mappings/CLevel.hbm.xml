<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eu.apenet.persistence.vo">
	<class name="CLevel" table="c_level" >
		<id name="clId" column="cl_id">
			<generator class="sequence">
				<param name="sequence">c_level_cl_id_seq</param>
			</generator>
		</id>
		<property name="hrefEadid" column="href_eadid" type="string" length="255"/>
		<property name="parentClId" column="parent_cl_id" />
		<property name="ecId" column="ec_id"/>
		<property name="leaf" column="leaf" type="boolean"/>
		<property name="orderId" column="order_id" type="int"/>
		<many-to-one name="eadContent" column="ec_id" insert="false" update="false"/>
		<property name="unittitle" column="unittitle" type="string"  length="255"/>
		<property name="unitid" column="unitid" type="string"  length="255"/>
		<property name="level" column="level" type="string" length="50"/>
		<many-to-one name="parent" column="parent_cl_id" insert="false" update="false" />
		<property name="xml" column="xml" type="text"/>
	</class>

</hibernate-mapping>