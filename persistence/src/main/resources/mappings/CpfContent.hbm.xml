<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eu.apenet.persistence.vo">
	<class name="CpfContent" table="cpf_content">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">cpf_content_id_seq</param>
			</generator>
		</id>
		<many-to-one name="archivalInstitution" class="eu.apenet.persistence.vo.ArchivalInstitution" column="ai_id" fetch="select" />
		<property name="cpfId" column="cpf_id" type="string" length="255"/>
		<property name="xml" column="xml" type="text"/>
	</class>
</hibernate-mapping>