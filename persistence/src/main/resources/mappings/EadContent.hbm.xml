<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eu.apenet.persistence.vo">
	<class name="EadContent" table="ead_content">
		<id name="ecId" column="ec_id">
			<generator class="sequence">
				<param name="sequence">ead_content_ec_id_seq</param>
			</generator>
		</id>
		<property name="faId" column="fa_id" />
		<many-to-one name="findingAid" column="fa_id" insert="false" update="false" />
		<property name="hgId" column="hg_id" />
		<many-to-one name="holdingsGuide" column="hg_id" insert="false" update="false" />
        <property name="sgId" column="sg_id" />
		<many-to-one name="sourceGuide" column="sg_id" insert="false" update="false" />
		<property name="titleproper" column="titleproper" type="text"/>
		<property name="eadid" column="eadid" type="string" length="255"/>
		<property name="unittitle" column="unittitle" type="text"/>
		<property name="visible" column="visible" type="boolean"/>
		<property name="xml" column="xml" type="text"/>
		<property name="displayIntro" column="display_intro" type="boolean"/>
		<property name="displayDid" column="display_did" type="boolean"/>				
	</class>
</hibernate-mapping>